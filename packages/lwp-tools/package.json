{
  "name": "create-ts-library",
  "version": "3.0.68",
  "description": "Javascript module template.",
  "main": "index.js",
  "scripts": {
    "setup": "yarn install --ignore-optional && yarn run flow-typed",
    "lint": "eslint --ext js,jsx config/ test/ *.js",
    "flow-typed": "echo ===== deprecated for create-ts-library =====",
    "flow": "echo ===== deprecated for create-ts-library =====",
    "flow:build": "echo ===== deprecated for create-ts-library =====",
    "babel": "babel ./config --ignore=node_modules --out-dir ./config",
    "webpack": "babel-node --plugins=transform-es2015-modules-commonjs ./node_modules/.bin/webpack --progress",
    "test": "yarn run lint && yarn run flow && yarn run test-babel && yarn run test-webpack",
    "build": "yarn run test && yarn run babel && yarn run webpack && yarn run flow:build",
    "release": "./publish.sh",
    "release:patch": "./publish.sh patch",
    "release:minor": "./publish.sh minor",
    "release:major": "./publish.sh major",
    "clean": "rimraf dist coverage",
    "reinstall": "rimraf flow-typed && rimraf node_modules && yarn install --ignore-optional",
    "debug": "cross-env NODE_ENV=debug",
    "prepublish": "in-publish && yarn run babel || not-in-publish",
    "postpublish": "git checkout config",
    "test-babel": "babel --config-file ./config/.babelrc-node test/* > /dev/null && babel --config-file ./config/.babelrc-browser test/* > /dev/null",
    "test-webpack": "babel-node --plugins=transform-es2015-modules-commonjs ./node_modules/.bin/webpack --progress",
    "check-updates": "npm-check-updates"
  },
  "browserslist": [
    "IE >= 9",
    "chrome >= 27",
    "firefox >= 30",
    "safari >= 5",
    "opera >= 23"
  ],
  "files": [
    "config",
    "declarations.js"
  ],
  "repository": "git://github.com/BurtHarris/grumbler-scripts.git",
  "keywords": [
    "template"
  ],
  "licenses": [
    {
      "type": "Apache 2.0",
      "url": "http://www.apache.org/licenses/LICENSE-2.0.html"
    }
  ],
  "readmeFilename": "README.md",
  "dependencies": {
    "@babel/cli": "^7",
    "@babel/core": "^7",
    "@babel/node": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7",
    "@babel/plugin-syntax-object-rest-spread": "^7",
    "@babel/plugin-transform-react-jsx": "^7",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/polyfill": "^7",
    "@babel/preset-env": "^7",
    "@babel/preset-react": "^7",
    "@babel/register": "^7.9.0",
    "@babel/runtime": "^7.9.2",
    "babel-loader": "^8.1.0",
    "babel-plugin-add-module-exports": "^1.0.2",
    "babel-plugin-istanbul": "^6.0.0",
    "babel-plugin-react-scoped-css": "^1.0.0",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-for-of": "^6.23.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-plugin-transform-es3-member-expression-literals": "^6.22.0",
    "babel-plugin-transform-es3-property-literals": "^6.22.0",
    "cache-loader": "^4.1.0",
    "circular-dependency-plugin": "^5.2.0",
    "css-loader": "^3.4.2",
    "eslint": "^6",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-plugin-compat": "^3.5.1",
    "eslint-plugin-const-immutable": "^2.1.0",
    "eslint-plugin-eslint-comments": "^3.1.2",
    "eslint-plugin-import": "~2.20.2",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-security": "^1.4.0",
    "hard-source-webpack-plugin": "^0.13.1",
    "imports": "^1.0.0",
    "in-publish": "^2.0.1",
    "isomorphic-style-loader": "^5.1.0",
    "istanbul-instrumenter-loader": "^3.0.1",
    "mocha": "^7",
    "node-cleanup": "^2.1.2",
    "node-sass": "^4.13.1",
    "npm-check-updates": "^4.1.1",
    "querystring": "^0.2.0",
    "raw-loader": "^4.0.0",
    "rimraf": "^3.0.2",
    "sass-loader": "^8.0.2",
    "scoped-css-loader": "^1.0.0",
    "semver": "^7.1.3",
    "terser-webpack-plugin": "^2.3.5",
    "text-loader": "0.0.1",
    "webpack": "^4.42.1",
    "webpack-bundle-analyzer": "^3.6.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "yargs": "^15.3.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.2.4",
    "cross-env": "^7.0.2",
    "eslint-plugin-unicorn": "^18.0.1",
    "jest": "^25.2.4"
  },
  "author": "Burt Harris <Burt_Harris@hotmail.com>",
  "license": "MIT"
}
